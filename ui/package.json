{
  "name": "suna-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:web": "concurrently \"VITE_RUNTIME=web vite\" \"nodemon --watch ../dsp/src -e mbt --exec 'npm run build:dsp --prefix ..'\"",
    "dev:juce": "concurrently \"vite\" \"nodemon --watch ../dsp/src -e mbt --exec 'npm run build:dsp --prefix ..'\"",
    "build:web": "VITE_RUNTIME=web vue-tsc -b && VITE_RUNTIME=web vite build --outDir dist-web",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "concurrently": "^9.2.1",
    "jsdom": "^27.4.0",
    "nodemon": "^3.1.11",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-singlefile": "^2.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^2.0.0"
  }
}
